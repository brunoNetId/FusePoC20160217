<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<mappings xmlns="http://dozer.sourceforge.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://dozer.sourceforge.net http://dozer.sourceforge.net/schema/beanmapping.xsd">
    <configuration>
        <wildcard>false</wildcard>
    </configuration>
    <mapping>
        <class-a>generated.GetCustomersResponse</class-a>
        <class-b>jsoninput.JsonInput</class-b>
        <field>
            <a>customer</a>
            <b>PublicList</b>
        </field>
    </mapping>
    <mapping>
        <class-a>generated.TypeCustomer</class-a>
        <class-b>jsoninput.PublicList</class-b>
        <field>
            <a>email</a>
            <b>email</b>
        </field>
        <field>
            <a>name</a>
            <b>name</b>
        </field>
    </mapping>
</mappings>
