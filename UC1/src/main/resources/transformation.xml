<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<mappings xmlns="http://dozer.sourceforge.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://dozer.sourceforge.net http://dozer.sourceforge.net/schema/beanmapping.xsd">
    <configuration>
        <wildcard>false</wildcard>
        <variables>
            <variable name="Title">Mr.</variable>
        </variables>
    </configuration>
    <mapping>
        <class-a>generated.Person</class-a>
        <class-b>generated.Person</class-b>
        <field>
            <a>city</a>
            <b>city</b>
        </field>
        <field>
            <a>lastName</a>
            <b>lastName</b>
        </field>
        <field>
            <a>user</a>
            <b>user</b>
        </field>
        <field>
            <a>firstName</a>
            <b>firstName</b>
        </field>
    </mapping>
    <mapping>
        <class-a>org.apache.camel.component.dozer.ExpressionMapper</class-a>
        <class-b>generated.Person</class-b>
    </mapping>
</mappings>
